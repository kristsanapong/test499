<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TechnicianActivity">

    <Button
        android:id="@+id/Account_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="8dp"
        android:onClick="UserData"
        android:text="จัดการข้อมูลผู้ใช้งาน"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/News_Button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="8dp"
        android:onClick="NewsData"
        android:text="จัดการข้อมูลประชาสัมพันธ์"
        app:layout_constraintEnd_toEndOf="@+id/Account_Button"
        app:layout_constraintStart_toStartOf="@+id/Account_Button"
        app:layout_constraintTop_toBottomOf="@+id/Account_Button" />


</android.support.constraint.ConstraintLayout>